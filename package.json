{
  "name": "m-ui",
  "version": "0.0.19",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "dev": "vue-styleguidist server",
    "doc": "vue-styleguidist build && vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "gh-pages": "gh-pages -d ./site",
    "version": "standard-version",
    "release": "npm run version  && git push --follow-tags && npm run doc && npm run gh-pages"
  },
  "dependencies": {
    "async-validator": "^3.4.0",
    "axios": "^0.21.0",
    "clipboard": "^2.0.6",
    "docx2html": "git+https://git.mingchao.com/git/mcwebfe-deps/docx2html.git#v1.3.4",
    "el-table-infinite-scroll": "^1.0.10",
    "wangeditor": "^4.2.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "chai": "^4.1.2",
    "commitizen": "^4.1.2",
    "core-js": "^3.6.5",
    "cz-conventional-changelog": "^3.2.0",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "gh-pages": "^3.1.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.20",
    "m-ui": "file:./",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "replace": "^1.2.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "standard-version": "^9.0.0",
    "vue": "^2.6.11",
    "vue-router": "^3.4.7",
    "vue-styleguidist": "^4.31.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "_id": "m-ui@0.1.0",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "replace 'https://git.mingchao.com///compare' 'https://git.mingchao.com/mcwebfe-deps/m-ui/compare' CHANGELOG.md && replace 'https://git.mingchao.com///commit' 'https://git.mingchao.com/mcwebfe-deps/m-ui/commit' CHANGELOG.md"
    }
  }
}
